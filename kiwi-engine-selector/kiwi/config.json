{
  "app": {
    "host": "engine",
    "port": 8000
  },
  "heuristics": {
    "system_age": {
      "module": "kiwi.heuristics.system_age"
    },
    "daytime": {
      "module": "kiwi.heuristics.daytime"
    },
    "user_vote_number": {
      "module": "kiwi.heuristics.user_vote_number",
      "arguments": ["user"]
    },
    "overall_user_mood": {
      "module": "kiwi.heuristics.overall_user_mood",
      "arguments": ["user"]
    },
    "algorithm_user_mood": {
      "module": "kiwi.heuristics.algorithm_user_mood",
      "arguments": ["user", "algorithm"]
    },
    "user": {
      "module": "kiwi.heuristics.user",
      "arguments": ["user"]
    }
  },
  "recommenders": {
    "random": "http://content:8000"
  }
}
